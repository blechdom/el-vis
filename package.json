{
  "name": "el-vis-audio",
  "version": "0.1.12",
  "homepage": "https://blechdom.github.io/el-vis-audio/",
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@elemaudio/core": "^1.0.8",
    "@elemaudio/web-renderer": "^1.0.15",
    "@storybook/addon-storyshots": "^6.5.12",
    "@storybook/client-api": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "auto": "^10.37.6",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "release": "auto shipit --base-branch=main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-named-exports-order": "^0.0.2",
    "gh-pages": "^4.0.0",
    "prop-types": "^15.8.1",
    "webpack": "^5.74.0"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "description": "## audio/visual design system for [elementary.audio](https:/elementary.audio)",
  "main": "index.tsx",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blechdom/el-vis-audio.git"
  },
  "keywords": [
    "audio",
    "visual",
    "design",
    "system",
    "elementary.audio"
  ],
  "author": "tbd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blechdom/el-vis-audio/issues"
  }
}
